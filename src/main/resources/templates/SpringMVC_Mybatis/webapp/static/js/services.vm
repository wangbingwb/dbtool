=%=(function(){
    =%=.services = {};
    =%=.url = "/ajax";
    =%=.extend(=%=.services,{
        example:function(data){
            return =%=.ajax({
                type: 'POST',
                url: =%=.url+"?method=ajax.${moduleName}.example",
                data: JSON.stringify(data),
                contentType:"text/plain",
                dataType: "json"
            })
        },
#foreach($table in $db.tables)
        create${table.getCName()}:function(data){
            return =%=.ajax({
                type: 'POST',
                url: =%=.url+"?method=ajax.${moduleName}.create.${table.getLName()}",
                data: JSON.stringify(data),
                contentType:"text/plain",
                dataType: "json"
            })
        },
        delete${table.getCName()}:function(data){
            return =%=.ajax({
                type: 'POST',
                url: =%=.url+"?method=ajax.${moduleName}.delete.${table.getLName()}",
                data: JSON.stringify(data),
                contentType:"text/plain",
                dataType: "json"
            })
        },
        update${table.getCName()}:function(data){
            return =%=.ajax({
                type: 'POST',
                url: =%=.url+"?method=ajax.${moduleName}.update.${table.getLName()}",
                data: JSON.stringify(data),
                contentType:"text/plain",
                dataType: "json"
            })
        },
        find${table.getCName()}:function(data){
            return =%=.ajax({
                type: 'POST',
                url: =%=.url+"?method=ajax.${moduleName}.find.${table.getLName()}",
                data: JSON.stringify(data),
                contentType:"text/plain",
                dataType: "json"
            })
        },
        get${table.getCName()}:function(data){
            return =%=.ajax({
                type: 'POST',
                url: =%=.url+"?method=ajax.${moduleName}.get.${table.getLName()}",
                data: JSON.stringify(data),
                contentType:"text/plain",
                dataType: "json"
            })
        },
        getAll${table.getCName()}:function(data){
            return =%=.ajax({
                type: 'POST',
                url: =%=.url+"?method=ajax.${moduleName}.get.all.list.${table.getLName()}",
                data: JSON.stringify(data),
                contentType:"text/plain",
                dataType: "json"
            })
        },
#end
    })
})