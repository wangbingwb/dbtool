<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane fx:controller="com.wb.dbtool.ctrl.DbDetailController" maxHeight="-Infinity" maxWidth="-Infinity"
          minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.65"
          xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="80.0" minWidth="0.0" prefWidth="80.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="140.0" minWidth="0.0" prefWidth="140.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="20.0" minWidth="0.0" prefWidth="20.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="50.0" minWidth="0.0" prefWidth="50.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="140.0" minWidth="0.0" prefWidth="140.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="20.0" minWidth="0.0" prefWidth="20.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label text="数据库名称" GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
        <Label text="数据库注释" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER"/>
        <Label text="前缀" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER"/>
        <Label text="域名" GridPane.halignment="CENTER" GridPane.rowIndex="0" GridPane.columnIndex="3" GridPane.valignment="CENTER"/>
        <Label text="模块" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.columnIndex="3"  GridPane.valignment="CENTER"/>
        <Label text="作者" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.columnIndex="3" GridPane.valignment="CENTER"/>
        <TextField GridPane.columnIndex="1" fx:id="dbname"/>
        <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" fx:id="dbcomment"/>
        <TextField GridPane.columnIndex="1" GridPane.rowIndex="2" fx:id="dbprefix"/>
        <TextField GridPane.columnIndex="4" GridPane.rowIndex="0" fx:id="basePackage"/>
        <TextField GridPane.columnIndex="4" GridPane.rowIndex="1" fx:id="moduleName"/>
        <TextField GridPane.columnIndex="4" GridPane.rowIndex="2" fx:id="author"/>
        <Label prefHeight="15.0" prefWidth="69.0" text="*" textFill="RED" GridPane.columnIndex="2"
               GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
        <Label prefHeight="15.0" prefWidth="56.0" text="*" textFill="RED" GridPane.columnIndex="2"
               GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER"/>
        <Label prefHeight="15.0" prefWidth="56.0" text="*" textFill="RED" GridPane.columnIndex="5"
               GridPane.halignment="CENTER" GridPane.rowIndex="0" GridPane.valignment="CENTER"/>
        <Label prefHeight="15.0" prefWidth="56.0" text="*" textFill="RED" GridPane.columnIndex="5"
               GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER"/>
    </children>

</GridPane>
